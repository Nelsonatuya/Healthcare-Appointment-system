 "html": "<div class=\"header\" style=\"text-align: center; border-bottom: 2px solid #333; padding-bottom: 10px; margin-bottom: 20px;\">\r\n    <h2>{{ frappe.get_doc(\"Company\", frappe.defaults.get_user_default(\"company\")).company_name or \"HEALTHCARE PRO CLINIC\" }}</h2>\r\n    <p>Clinical Consultation Report</p>\r\n</div>\r\n\r\n<div class=\"row\">\r\n    <div class=\"col-xs-6\">\r\n        <strong>Patient Name:</strong> {{ doc.patient }}<br>\r\n        <strong>Date:</strong> {{ frappe.format_date(doc.date) }}\r\n    </div>\r\n    <div class=\"col-xs-6 text-right\">\r\n        <strong>Doctor:</strong> {{ doc.practitioner }}<br>\r\n        <strong>Appointment ID:</strong> {{ doc.appointment }}\r\n    </div>\r\n</div>\r\n\r\n<hr>\r\n\r\n<h4>Clinical Findings</h4>\r\n<div style=\"margin-bottom: 20px;\">\r\n    <strong>Symptoms:</strong><br>\r\n    {{ doc.symptoms or \"None reported\" }}\r\n</div>\r\n\r\n<div style=\"margin-bottom: 20px;\">\r\n    <strong>Diagnosis:</strong><br>\r\n    {{ doc.diagnosis }}\r\n</div>\r\n\r\n<hr>\r\n\r\n<h4>Prescriptions</h4>\r\n<table class=\"table table-bordered\">\r\n    <thead>\r\n        <tr>\r\n            <th>No.</th>\r\n            <th>Medicine Name</th>\r\n            <th>Dosage</th>\r\n            <th>Instructions</th>\r\n        </tr>\r\n    </thead>\r\n    <tbody>\r\n        {% for item in doc.prescriptions %}\r\n        <tr>\r\n            <td>{{ loop.index }}</td>\r\n            <td><strong>{{ item.medicine }}</strong></td>\r\n            <td>{{ item.dosage }}</td>\r\n            <td>{{ item.instructions }}</td>\r\n        </tr>\r\n        {% endfor %}\r\n    </tbody>\r\n</table>\r\n\r\n<div class=\"footer\" style=\"margin-top: 50px; text-align: right;\">\r\n    <p>__________________________</p>\r\n    <p>Doctor's Signature</p>\r\n</div>",
